@inherits LayoutComponentBase

@inject SocketClient socketClient
@inject IJSRuntime js

<div class="page">
    <div class="sidebar">
        <NavMenu />
    </div>

    <main>
        <article class="content px-4">
            @Body
        </article>
    </main>
</div>

@code{
    protected override async Task OnInitializedAsync()
    {
        await socketClient.Initilize(js);
    }
}
